CREATE TABLE TratadosIntern(
	cod SERIAL,
	nombre_tratado TEXT,
	bilateral BOOL,
	lugar_adopción TEXT,
	fecha_adopción DATE,
	estados_organismos TEXT,
	temas TEXT,
	naturaleza_tratado TEXT,
	depositario TEXT,
	suscrib_colombia TEXT,
	vigente BOOL,
	fecha_ley_aprobatoria DATE,
	numero_ley_aprobatoria TEXT,
	sentencia_fecha_ley DATE,
	sentencia_numero TEXT,
	decreto_fecha_diario_oficial DATE,
	decreto_numero_diario_oficial TEXT,
	pais_tratado TEXT,
	codigo_llamada TEXT,
	capital TEXT,
	region TEXT,
	subregion TEXT,
	poblacion TEXT,
	area INTEGER,
	zona_horaria TEXT,
	monedas TEXT,
	idiomas TEXT,
	cantidad_fronteras INTEGER,
	diferencia_zona_hora DECIMAL,
	PRIMARY KEY(cod)	
)